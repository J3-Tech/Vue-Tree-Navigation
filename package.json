{
  "name": "vue-tree-navigation",
  "version": "4.0.2",
  "description": "Vue.js 2 tree navigation",
  "keywords": [
    "tree",
    "navigation",
    "vue",
    "vuejs",
    "component",
    "plugin",
    "menu"
  ],
  "homepage": "https://github.com/J3-Tech/vue-tree-navigation",
  "bugs": "https://github.com/J3-Tech/vue-tree-navigation/issues",
  "license": "MIT",
  "author": "Michaela Robošová <robosova.michaela@seznam.cz>",
  "files": [
    "dist"
  ],
  "main": "dist/vue-tree-navigation.js",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run unit"
    }
  },
  "engines": {
    "npm": ">=7.10.0 <=9.5.0",
    "node": ">=16.13.0 <=19.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/J3-Tech/Vue-Tree-Navigation.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "build:docs": "cross-env NODE_ENV=production TYPE=docs webpack",
    "lint:js": "eslint --ext .js,.vue .",
    "lint:ts": "ts-standard",
    "lint:ts:fix": "ts-standard --fix",
    "lint:scss": "stylelint './**/*.scss'",
    "lint": "yarn lint:ts & yarn lint:scss",
    "prettier-list": "prettier --list-different '**/*.{js,vue}' --ignore-path .eslintignore",
    "prettier": "prettier --write '**/*.{js,vue}' --ignore-path .eslintignore",
    "test": "jest --coverage src/",
    "e2e": "node ./e2e/runner.js"
  },
  "dependencies": {
    "husky": "^8.0.3",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/eslint-parser": "~7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@tsconfig/recommended": "^1.0.2",
    "@types/jest": "^29.4.2",
    "@types/jest-each": "^24.3.0",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/compiler-sfc": "^3.1.0",
    "@vue/test-utils": "^2.3.1",
    "@vue/tsconfig": "^0.1.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.3.0",
    "babel-preset-stage-3": "^6.24.1",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "cypress": "^12.8.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^9.9.0",
    "file-loader": "~6.2.0",
    "http-server": "~14.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "~26.6.3",
    "jest-each": "~26.6.2",
    "jest-environment-jsdom": "~26.6.2",
    "jest-plugin-context": "^2.6.0",
    "jest-serializer-vue": "^3.1.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "sass": "^1.59.3",
    "sass-loader": "~13.2.0",
    "shelljs": "^0.8.2",
    "source-map-loader": "^4.0.1",
    "start-server-and-test": "^2.0.0",
    "stylelint": "~15.3.0",
    "stylelint-config-recess-order": "~4.0.0",
    "stylelint-config-standard": "~30.0.0",
    "stylelint-processor-arbitrary-tags": "^0.1.0",
    "stylelint-scss": "~4.4.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.4.2",
    "ts-standard": "^12.0.2",
    "typescript": "^4.8.4",
    "vite": "^4.1.4",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.2.0",
    "vue-jest": "~5.0.0-alpha.10",
    "vue-loader": "~17.0.1",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.76.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.12.0"
  },
  "ts-standard": {
    "project": "./tsconfig.json",
    "ignore": [
      "dist",
      "*.js"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue",
      "ts"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest"
    },
    "moduleNameMapper": {
      "\\.(scss)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "setupFiles": [
      "jest-plugin-context/setup"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/index.js",
      "!src/plugin.js"
    ],
    "testEnvironment": "jest-environment-jsdom"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
