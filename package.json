{
  "name": "vue-tree-navigation",
  "version": "1.0.2",
  "description": "A Vue.js tree navigation",
  "keywords": [
    "tree",
    "navigation",
    "vue",
    "vuejs",
    "component",
    "plugin"
  ],
  "homepage": "https://github.com/MisRob/vue-tree-navigation",
  "bugs": "https://github.com/MisRob/vue-tree-navigation/issues",
  "license": "MIT",
  "author": "Michaela Robošová <robosova.michaela@seznam.cz>",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/MisRob/vue-tree-navigation.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot --content-base dev/",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "lint:js": "eslint --ext .js,.vue .",
    "lint:scss": "stylelint './**/*.scss'",
    "lint": "npm run lint:js & npm run lint:scss",
    "test": "jest",
    "test:verbose": "jest --verbose"
  },
  "dependencies": {
    "vue": "^2.5.11"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-config-genius": "^0.1.4",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1.1.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.0.4",
    "jest-plugin-context": "^2.6.0",
    "jest-serializer-vue": "^0.3.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "stylelint": "^8.4.0",
    "stylelint-config-recess-order": "^1.2.3",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^2.2.0",
    "vue-jest": "^1.4.0",
    "vue-loader": "^13.0.5",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.4.4",
    "vue-test-utils": "^1.0.0-beta.8",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "mapCoverage": true,
    "moduleNameMapper": {
      "\\.(scss)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "setupFiles": [
      "jest-plugin-context/setup"
    ]
  }
}
